            </div>
            
        </div>
        <!-- /#page-wrapper -->
<script type="text/javascript">
  
    $('.form_date').datetimepicker({
        format: 'yyyy-mm-dd',
        language:  'zh-CN',
        weekStart: 1,
        todayBtn:  1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        minView: 2,
        forceParse: 0
    }); 
    $('.clear_val').click(function(){
        $(this).siblings().val('')
    })
</script>

